{
  "version": 1,
  "disable_existing_loggers": false,
  "formatters": {
      "ecs": {
          "format": "%(message)s",
          "()": "ecs_logging.StdlibFormatter",
          "exclude_fields": [
              "color_message"
          ]
    }
  },
  "handlers": {
    "console": {
        "class": "logging.StreamHandler",
        "stream": "ext://sys.stdout",
        "formatter": "ecs"
    }
  },
  "loggers": {
  },
  "root": {
      "level": "INFO",
      "handlers": ["console"],
      "propagate": "no"
  }
}
